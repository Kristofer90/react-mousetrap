{
  "name": "react-mousetrap",
  "version": "0.1.1",
  "description": "React's higher order component for Mousetrap integration",
  "main": "lib/index.js",
  "readmeFilename": "README.md",
  "scripts": {
    "test": "jest",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/__tests__"
    ],
    "testPathIgnorePatterns": [
      "pending"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils"
    ],
    "modulePathIgnorePatterns": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvinsj/react-mousetrap.git"
  },
  "keywords": [
    "react",
    "mousetrap",
    "keyboard",
    "shortcuts",
    "events"
  ],
  "author": {
    "name" : "Alvin Ng",
    "email" : "email.to.alvin@gmail.com",
    "url" : "https://github.com/alvinsj"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alvinsj/react-mousetrap/issues"
  },
  "homepage": "https://github.com/alvinsj/react-mousetrap#readme",
  "dependencies": {
    "mousetrap": "^1.6.0"
  },
  "peerDependencies": { 
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-jest": "alvinsj/babel-jest#babel-6",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "jest-cli": "alvinsj/jest#quick-fix",
    "react-addons-test-utils": "^0.14.0 || ^15.0.0"
  }
}
